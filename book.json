{
    "name": "Landsu",
    "version": "0.0.0",
    "description": "Yesura",
    "main": "./index.js",
    "gitbook": "^3.2.3",
    "gitbook-cli": "^2.3.2",
    


    "root": "./content",
    "output": "./docs",
    "title": "Landsu",
    "plugins": [
      "-lunr", "-search", "search-pro",
      "fontsettings", 
      "-highlight", "prism", "prism-themes",
      "styled-blockquotes",
      "copy-code-button",
      "cuav-chapters",
      "anchor-navigation-ex-toc"
    ],
    "pluginsConfig": {
      "prism": {
          "css": ["prismjs/themes/prism-okaidia.css"]
       },
       "anchor-navigation-ex-toc": {
        "showLevel": false
       }
    },
    
    
    "styles": {
      "website": "styles/website.css",
      "ebook": "styles/ebook.css",
      "pdf": "styles/pdf.css",
      "mobi": "styles/mobi.css",
      "epub": "styles/epub.css"
    },
    

    
    "variables": {
        "version": "1.0.0"
    }
}  